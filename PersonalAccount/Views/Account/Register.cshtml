@model PersonalAccount.Models.LogRegViewModel
@{
    ViewBag.Title = "ДельтаКредит - Регистрация";
}
<div class="wrapper">
    <div class="container">
        <div class="row">
            <div class="col-md-1">
            </div>
            <div class="col-md-8" id="Register_Partial">
                @if (ViewBag.Type == "RegisterOGRN")
                {
                    @Html.Partial("~/Views/Account/RegisterOGRN.cshtml")
                }
                else if (ViewBag.Type == "NoRegistration")
                {
                    @Html.Partial("~/Views/Account/NoRegistration.cshtml")
                }
                else if (ViewBag.Type == "RegisterUser")
                {
                    @Html.Partial("~/Views/Account/RegisterUser.cshtml")
                }   
                else if (ViewBag.Type == "CompanyRegistered")
                {
                    @Html.Partial("~/Views/Account/CompanyRegistered.cshtml")
                }
                else if (ViewBag.Type == "RegisterBranch")
                {
                    @Html.Partial("~/Views/Account/RegisterBranch.cshtml")
                }
                else if (ViewBag.Type == "RegisterSuccess")
                {
                    @Html.Partial("~/Views/Account/RegisterSuccess.cshtml")
                }
                else
                {
                    @Html.Partial("~/Views/Account/RegisterQuestion.cshtml")
                }
            </div>
            <div class="col-md-3" id="Login_Partial">
                @Html.Partial("~/Views/Account/Login.cshtml")
            </div>
        </div>
    </div>
</div>

